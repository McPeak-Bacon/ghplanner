    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.0",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^11.0.0",
    "clsx": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.300.0",
    "next": "15.5.2",
    "next-auth": "^4.24.11",
    "postcss": "^8.4.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "resend": "^4.0.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.0.0"
